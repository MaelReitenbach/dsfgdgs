
<table class="table">
  <thead>
    <tr>
      <th scope="col">Email list</th>
      <th scope="col">Selected email</th>
    </tr>
  </thead>
  <tbody>
    <% Email.all.each do |email| %>
    <tr id="<%= email.id %>">
            <td id="<%= email.object %>-<%= email.id %>" class="collapse"></td>
      <td id="left-<%= email.id %>"><b><%= link_to email.object, email_path(email.id),  remote: true %></b></td>

    </tr>
<% end %>
  </tbody>
</table>
<%= button_to "Get an email", emails_path, :method => :create, class:"btn btn-primary", remote: true %>